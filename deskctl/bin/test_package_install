#!/usr/bin/python

import Pyro4
from time import sleep

deskctld = Pyro4.Proxy('PYRO:deskctld@./u:' + "/run/deskctld.sock")
deskctld.pkgInstall(["inkscape","saidar","libstatgrab","fftw.x86_64","netcdf","fftw.i686"])
print deskctld.pkgQueueList()
deskctld.pkgRemove(["inkscape","saidar","libstatgrab","fftw","netcdf"])
print deskctld.pkgQueueList()
deskctld.pkgGroupInstall(["Emacs"])
print deskctld.pkgQueueList()
deskctld.pkgGroupRemove(["Emacs"])
print deskctld.pkgQueueList()

sleep(5)
print deskctld.pkgQueueList()

